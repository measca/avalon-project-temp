// 将设置放入此文件中以覆盖默认值和用户设置。
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/node_modules": true,
    "**/.vscode": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/node_modules": true,
    "**/.vscode": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.vscode": true
  }
}